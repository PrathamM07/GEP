{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "white-paper-details-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/whitePaperDetails/WhitePaperDetailsWebPart.js",
          "manifest": "./src/webparts/whitePaperDetails/WhitePaperDetailsWebPart.manifest.json"
        }
      ]
    },
    "body-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/body/BodyApplicationCustomizer.js",
          "manifest": "./src/extensions/body/BodyApplicationCustomizer.manifest.json"
        }
      ]
    },
    "gep-listing-page-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/gepListingPage/GepListingPageWebPart.js",
          "manifest": "./src/webparts/gepListingPage/GepListingPageWebPart.manifest.json"
        }
      ]
    },
    "imagegallery-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/imagegallery/ImagegalleryWebPart.js",
          "manifest": "./src/webparts/imagegallery/ImagegalleryWebPart.manifest.json"
        }
      ]
    },
    "upcoming-events-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/upcomingEvents/UpcomingEventsWebPart.js",
          "manifest": "./src/webparts/upcomingEvents/UpcomingEventsWebPart.manifest.json"
        }
      ]
    },
    "radio-spots-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/radioSpots/RadioSpotsWebPart.js",
          "manifest": "./src/webparts/radioSpots/RadioSpotsWebPart.manifest.json"
        }
      ]
    }
  },
  "localizedResources": {
    "WhitePaperDetailsWebPartStrings": "lib/webparts/whitePaperDetails/loc/{locale}.js",
    "BodyApplicationCustomizerStrings": "lib/extensions/body/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "GepListingPageWebPartStrings": "lib/webparts/gepListingPage/loc/{locale}.js",
    "ImagegalleryWebPartStrings": "lib/webparts/imagegallery/loc/{locale}.js",
    "UpcomingEventsWebPartStrings": "lib/webparts/upcomingEvents/loc/{locale}.js",
    "RadioSpotsWebPartStrings": "lib/webparts/radioSpots/loc/{locale}.js"
  }
}
